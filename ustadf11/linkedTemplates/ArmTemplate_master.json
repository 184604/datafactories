{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ustadf11"
		},
		"Cassandra1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Cassandra1'"
		},
		"AzureTableStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureTableStorage'"
		},
		"Cassandra1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "ustsa099"
		},
		"Cassandra1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "azureadmin"
		},
		"SWAPI Service_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://swapi.dev/api/people"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ustadf11_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Cassandra1_password": {
						"value": "[parameters('Cassandra1_password')]"
					},
					"AzureTableStorage_connectionString": {
						"value": "[parameters('AzureTableStorage_connectionString')]"
					},
					"Cassandra1_properties_typeProperties_host": {
						"value": "[parameters('Cassandra1_properties_typeProperties_host')]"
					},
					"Cassandra1_properties_typeProperties_username": {
						"value": "[parameters('Cassandra1_properties_typeProperties_username')]"
					},
					"SWAPI Service_properties_typeProperties_url": {
						"value": "[parameters('SWAPI Service_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}